{
  "name": "dcanary-cli",
  "version": "1.0.0",
  "description": "Dcanary - Decentralized CI/CD Pipeline for Internet Computer Projects",
  "main": "dist/index.js",
  "bin": {
    "dcanary": "dist/index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run test",
    "lint": "eslint src/**/*.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "publish:npm": "npm publish",
    "publish:beta": "npm publish --tag beta"
  },
  "keywords": [
    "cli",
    "ci-cd",
    "cicd",
    "pipeline",
    "decentralized",
    "build",
    "deployment",
    "verification",
    "consensus",
    "blockchain",
    "internet-computer",
    "icp",
    "canister",
    "motoko",
    "rust",
    "azle",
    "dfx",
    "web3",
    "devops",
    "automation",
    "typescript"
  ],
  "author": "Daniel Okinda <modaniels@email.com>",
  "license": "MIT",
  "dependencies": {
    "@dfinity/agent": "^2.1.1",
    "@dfinity/candid": "^2.1.0",
    "@dfinity/identity": "^2.1.1",
    "@dfinity/principal": "^2.1.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "inquirer": "^8.2.6",
    "joi": "^17.11.0",
    "node-forge": "^1.3.1",
    "ora": "^5.4.1",
    "semver": "^7.5.0",
    "winston": "^3.13.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/modaniels/Dcanary.git"
  },
  "bugs": {
    "url": "https://github.com/modaniels/Dcanary/issues"
  },
  "homepage": "https://dcanary.vercel.app",
  "engines": {
    "node": ">=16.0.0"
  }
}
